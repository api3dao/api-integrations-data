{
  "name": "PYUSD/USD",
  "path": "v4/timeseries/asset-metrics",
  "parameters": {
    "assets": "pyusd",
    "metrics": "ReferenceRate",
    "frequency": "1s",
    "limit_per_asset": "1",
    "page_size": "2000"
  },
  "parser": "(response) => {\n          return response.data.find((d) => d.asset === 'pyusd').ReferenceRate\n        }\n        "
}
